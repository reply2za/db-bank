  "main": "src/main/index.ts",
  "scripts": {
    "start": "tsc && node dist/src/main/index.js",
    "build": "tsc",
    "dev": "tsc && node --trace-warnings -r source-map-support/register dist/src/main/index.js --dev",
    "prod": "npm run pm2",
    "prod:build": "npm install && tsc -p tsconfig.prod.json",
    "pm2": "bash scripts/pm2.bash",
    "release:patch": "npm run test && npm version patch -m \"chore(release): %s\"",
    "release:minor": "npm run test && npm version minor -m \"chore(release): %s\"",
    "test": "tsc && jest --detectOpenHandles --forceExit"
  },
