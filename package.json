  "main": "src/main/index.ts",
  "scripts": {
    "start": "node --env-file .env dist/src/main/index.js",
    "start:dev": "node --env-file .env --trace-warnings -r source-map-support/register dist/src/main/index.js --dev",
    "build:dev": "tsc",
    "build:prod": "npm install && tsc -p tsconfig.prod.json",
    "dev": "npm run build:dev && npm run start:dev",
    "prod": "bash scripts/pm2.bash",
    "release:patch": "npm run test && npm version patch -m \"chore(release): %s\"",
    "release:minor": "npm run test && npm version minor -m \"chore(release): %s\"",
    "test": "tsc && jest --detectOpenHandles --forceExit"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "author": "Reply2za",
  "license": "ISC",
  "dependencies": {
    "@hoursofza/djs-common": "~1.1.2",
    "@types/uuid": "^9.0.0",
    "axios": "^1.3.4",
    "discord.js": "~14.13.0",
    "fs": "^0.0.1-security",
    "leven": "^3.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/request": "^2.48.8",
    "jest": "^29.6.0",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  }
