    "fs": "^0.0.1-security",
    "leven": "^3.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/request": "^2.48.8",
    "jest": "^29.5.0",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
